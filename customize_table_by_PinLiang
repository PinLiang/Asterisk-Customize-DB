CREATE TABLE peers(
PRIMARY KEY(id),
id int(11) NOT NULL auto_increment,
name varchar(10) NOT NULL default '',
registersip char(3) NOT NULL default 'no',
host varchar(7) NOT NULL default 'dynamic',
callgroup int(2) NOT NULL default '1',
mailbox varchar(10) NOT NULL default '',
`call-limit` int(11) NOT NULL default '100',
type varchar(10) NOT NULL default 'peer',
username varchar(10) NOT NULL default '',
transfer char(3) NOT NULL default 'yes',
callcounter char(3) NOT NULL default 'yes',
context varchar(20) NOT NULL default 'DLPN_DialPlan1',
cid_number varchar(10) NOT NULL default '',
hasvoicemail char(3) NOT NULL default 'no',
vmsecret varchar(10) default NULL,
email varchar(50) default NULL,
threewaychalling varchar(3) NOT NULL default 'no',
hasdirectory char(3) NOT NULL default 'no',
callwaiting char(3) NOT NULL default 'no',
hasmanager char(3) NOT NULL default 'no',
hasagent char(3) NOT NULL default 'no',
hassip char(3) NOT NULL default 'yes',
hasiax char(3) NOT NULL default 'no',
secret varchar(30) NOT NULL default '',
nat char(3) NOT NULL default 'yes',
canreinvite char(3) NOT NULL default 'no',
dtmfmode varchar(7) NOT NULL default 'rfc2833',
insecure char(3) NOT NULL default 'no',
pickupgroup int(2) NOT NULL default '1',
disallow varchar(3) NOT NULL default 'all',
allow varchar(50) NOT NULL default 'ulaw, gsm',
macaddress varchar(10) NOT NULL default '',
autoprov char(3) NOT NULL default 'yes',
label varchar(10) NOT NULL default '',
linenumber int(1) NOT NULL default '1',
LINKKEYS int(1) NOT NULL default '1',
ipaddr varchar(15) NOT NULL default ''
);
